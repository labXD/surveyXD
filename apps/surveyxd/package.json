{
  "name": "surveyxd",
  "private": true,
  "version": "0.0.1-dev",
  "scripts": {
    "dev": "concurrently pnpm:dev:db pnpm:dev:app",
    "dev:app": "wait-on tcp:3306 && pnpm run db:gen && pnpm run dev:next",
    "dev:db": "pscale connect surveyxd dev --org labxd",
    "build": "next build",
    "start": "next start",
    "dev:next": "next dev",
    "typecheck": "tsc --noEmit",
    "docs:gen": "typedoc --plugin typedoc-plugin-markdown --out docs src --excludeNotDocumented --entryPointStrategy expand",
    "db:pull": "prisma db pull",
    "db:gen": "prisma generate",
    "db:push": "prisma db push",
    "db:push:cloud": "wait-on tcp:3306 && pnpm run db:push && pnpm run db:gen",
    "db:format": "prisma db format",
    "db:migrate:dev": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:deploy": "prisma migrate deploy"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.0",
    "@labxd/gustxd": "^0.6.0",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.3.1",
    "@trpc/client": "^9.27.2",
    "@trpc/next": "^9.27.2",
    "@trpc/react": "^9.27.2",
    "@trpc/server": "^9.27.2",
    "autoprefixer": "10.4.8",
    "clsx": "^1.2.1",
    "next": "12.3.0",
    "next-auth": "^4.10.3",
    "postcss": "8.4.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3",
    "tailwindcss": "3.1.8",
    "zod": "^3.18.0",
    "vega": "^5.22.1",
    "next-transpile-modules": "9.0.0",
    "next-compose-plugins": "2.2.1",
    "react-no-ssr": "1.1.0",
    "@msrvida/sanddance-react": "^4"
  },
  "devDependencies": {
    "@svgr/webpack": "6.3.1",
    "@types/node": "18.7.16",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "concurrently": "^7.3.0",
    "prisma": "^4.3.1",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.14",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "4.7.4",
    "wait-on": "^6.0.1",
    "@types/react-no-ssr": "1.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/labXD/surveyXD.git",
    "directory": "apps/surveyxd"
  }
}
