{
  "name": "surveyxd",
  "description": "Survey generator and data visualization",
  "private": true,
  "version": "0.0.1-dev",
  "scripts": {
    "dev": "concurrently pnpm:dev:db pnpm:dev:app",
    "dev:clean": "rm -rf .next && pnpm run dev",
    "dev:app": "wait-on tcp:3306 && pnpm run db:gen && pnpm run dev:next",
    "dev:db": "pscale connect surveyxd dev --org labxd",
    "build": "next build",
    "start": "next start",
    "dev:next": "next dev",
    "typecheck": "tsc --noEmit",
    "docs:gen": "typedoc --plugin typedoc-plugin-markdown --out docs src --excludeNotDocumented --entryPointStrategy expand",
    "db:pull": "prisma db pull",
    "db:gen": "prisma generate",
    "db:push": "prisma db push",
    "db:push:cloud": "wait-on tcp:3306 && pnpm run db:push && pnpm run db:gen",
    "db:format": "prisma db format",
    "db:migrate:dev": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:deploy": "prisma migrate deploy"
  },
  "dependencies": {
    "@headlessui/react": "1.7.2",
    "@headlessui/tailwindcss": "0.1.1",
    "@hookform/resolvers": "2.9.8",
    "@labxd/gustxd": "^0.7.2",
    "@msrvida/sanddance-react": "^4",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.3.1",
    "@trpc/client": "^9.27.2",
    "@trpc/next": "^9.27.2",
    "@trpc/react": "^9.27.2",
    "@trpc/server": "^9.27.2",
    "autoprefixer": "10.4.11",
    "clsx": "^1.2.1",
    "next": "12.3.0",
    "next-auth": "^4.10.3",
    "next-compose-plugins": "2.2.1",
    "next-transpile-modules": "9.0.0",
    "postcss": "8.4.16",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.35.0",
    "react-query": "3",
    "sharp": "^0.31.0",
    "tailwindcss": "3.1.8",
    "vega": "^5.22.1",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@svgr/webpack": "6.3.1",
    "@types/node": "18.7.18",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-no-ssr": "1.1.3",
    "concurrently": "7.4.0",
    "prisma": "^4.3.1",
    "trpc-playground": "^0.4.3",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.14",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "4.7.4",
    "wait-on": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/labXD/surveyXD.git",
    "directory": "apps/surveyxd"
  }
}
